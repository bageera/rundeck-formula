rundeck:
  ## Default values
  config:
    host: localhost
    port: 4440
    projects:
      storage: filesystem
      dir: /var/rundeck/projects
      defaults:
        resources:
          - source: file
            format: resourcexml
            file: /var/rundeck/projects/${project.name}/etc/resources.xml
            generate_file_automatically: True
            include_server_node: True
            require_file_exists: False
        node_executor: jsch-ssh
        file_copier: jsch-scp
        node_executors:
          jsch-ssh:
            authentication: privateKey
            key:
              file_path: /var/lib/rundeck/.ssh/id_rsa
              storage_path:
            password:
          script:
            command:
            interpreter:
            directory:
        file_copiers:
          jsch-scp:
            authentication: privateKey
            key:
              file_path: /var/lib/rundeck/.ssh/id_rsa
              storage_path:
            password:
          script:
            command:
            remote_file_path:
            interpreter:
            directory:

  # Manage users
  users:
    admin:
      # Using MD5 hashed password generated with org.eclipse.jetty.util.security.Password
      password: MD5:21232f297a57a5a743894a0e4a801fc3
      groups:
        - admin
        - user
    user:
      # Using plain text password
      password: plaintextpassword
      groups:
        - user

  # Manage projects
  projects:
    my-project:
      resources:
        - source: directory
          directory: /var/rundeck/projects/${project.name}/etc/resources
        - source: script
          format: resourceyaml
          script: /var/rundeck/projects/${project.name}/etc/get-resources.sh
          interpreter: bash
          arguments:
          quote_interpreter_args: False
        - source: url
          url: http://myserver/nodes.yml
          timeout: 30
          cache_results: True
        - source: stub
          prefix: node
          suffix:
          count: 1
          tags: stub
        - source: pillar
          nodes:
            my-server:
              hostname: 192.168.0.1
              username: rundeck
      node_executor: script
      file_copier: script
